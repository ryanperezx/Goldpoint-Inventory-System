<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Goldpoint_Inventory_System"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="Goldpoint_Inventory_System.PrintCopies"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Print Copies" Height="200" Width="400">
    <Grid>
        <Label Content="Print how many copies?:" HorizontalAlignment="Left" Margin="57,40,0,0" VerticalAlignment="Top"/>
        <syncfusion:IntegerTextBox x:Name="txtCopies" MinValue="1" HorizontalAlignment="Left" Height="26" Margin="200,40,0,0" MinWidth="100" NumberGroupSeparator="," TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="100"/>
        <syncfusion:ButtonAdv SmallIcon="{x:Null}" x:Name="btnPrint" Click="BtnPrint_Click" HorizontalAlignment="Left" Height="29" Label="Print" Margin="140,115,0,0" VerticalAlignment="Top" Width="97"/>
    </Grid>
</Window>
